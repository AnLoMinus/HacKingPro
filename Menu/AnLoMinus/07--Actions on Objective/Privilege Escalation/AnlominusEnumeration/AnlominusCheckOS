CheckOS(){
    KERNEL=$(uname -r)
    RES=$(xdpyinfo | grep dimensions | sed -r 's/^[^0-9]*([0-9]+x[0-9]+).*$/\1/')
    MEMORY=$(grep MemTotal /proc/meminfo | awk '{print $2}')
    CPU=$(lscpu | grep 'Model name' | cut -f 2 -d ":" | awk '{$1=$1}1')
    echo "\n  $cWallâ–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“$cWall"
    echo "  $cWallâ–“                                             â–“$cWall"
    echo "  $cWallâ–“  â•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—  â–“$cWall"
    echo "  $cWallâ–“  â• â•>         System Information       <==â•‘  â–“$cWall"
    echo "  $cWallâ–“  â• â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•  â–“$cWall"
    echo "  $cWallâ–“  â• â•â–¶${orange} Uname:${nc} $(uname)"
    echo "  $cWallâ–“  â• â•â–¶${orange} who is currently logged in:${nc} $(who)"
    echo "  $cWallâ–“  â• â•â–¶${orange} User:${nc} $USER"
    echo "  $cWallâ–“  â• â•â–¶${orange} Host:${nc} $(hostname)"
    echo "  $cWallâ–“  â• â•â–¶${orange} Host IP Address:${nc} $(hostname -i)"
    echo "  $cWallâ–“  â• â•â–¶${orange} Home:${nc} $HOME"
    echo "  $cWallâ–“  â• â•â–¶${orange} Term:${nc} ${TERM}"
    echo "  $cWallâ–“  â• â•â–¶${orange} CPU:${nc} $CPU"
    echo "  $cWallâ–“  â• â•â–¶${orange} Desktop Session:${nc} $DESKTOP_SESSION"
    echo "  $cWallâ–“  â• â•â–¶${orange} Kernel:${nc} $KERNEL"
    echo "  $cWallâ–“  â• â•â–¶${orange} Resolution:${nc} $RES"
    echo "  $cWallâ–“  â• â•â–¶${orange} Memory:${nc} $(( ${MEMORY%% *} / 1024)) MiB"
    echo "  $cWallâ–“  â• â•â–¶${orange} Script ID:${nc} $$"
    echo "  $cWallâ–“  â• â•â–¶${orange} System Language:${nc} ${LANG}"
    echo "  $cWallâ–“  â• â•â–¶${orange} Shell:${nc} $SHELL"
    echo "  $cWallâ–“  â• â•â–¶${orange} UpTime: ${nc} $(uptime)"
    echo "  $cWallâ–“  â• â•â–¶${orange} Date: ${nc} $(date)"
    echo "  $cWallâ–“  â• â•â–¶${orange} Path:${nc} $PATH"
    echo "  $cWallâ–“  â• â•â–¶${orange} User ID:${nc} $(id)"
    echo "  $cWallâ–“  â•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•  â–“$cWall"
    echo "  $cWallâ–“                                             â–“$cWall"
    echo "  $cWallâ–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“â–“$cWall\n"
