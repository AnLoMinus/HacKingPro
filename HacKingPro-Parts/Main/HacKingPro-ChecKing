#--â•¼[ ChecKing ]--â•¼â–° [ System ]
Check_System(){
  #--â•¼[ ChecKing ]--â•¼â–° [ System ]--â•¼â–° [ Architecture ]
  case `dpkg --print-architecture` in
    darwin-amd64)
    SystemArchitecture="darwin-amd64" ;;
    aarch64)
    SystemArchitecture="arm64" ;;
    arm)
    SystemArchitecture="arm" ;;
    armhf)
    SystemArchitecture="armhf" ;;
    amd64)
    SystemArchitecture="amd64" ;;
    i*86)
    SystemArchitecture="i386" ;;
    x86_64)
    SystemArchitecture="amd64" ;;
    *)
    echo "Architecture Not Found"
    SystemArchitecture=" " ;;
  esac

  #--â•¼[ ChecKing ]--â•¼â–° [ System ]--â•¼â–° [ Package Manager ]
  echo "\n"
  if [ -e /data/data/com.termux/files/usr/bin/pkg ]; then
    MyPackManager="pkg"
    MySystem="Termux"
  elif [ $(command -v apk) ]; then
    MyPackManager="apk"
      MySystem="Android"
  elif [ $(command -v brew) ]; then
    MyPackManager="brew"
    MySystem="MacOS"
    sudo=""
  elif [ $(command -v apt-get) ]; then
    MyPackManager="apt-get"
    MySystem="Linux"
  elif [ $(command -v apt) ]; then
    MyPackManager="apt"
    MySystem="Linux"
  elif [ $(command -v yum) ]; then
    MyPackManager="yum"
    MySystem="Fedora"
  fi

  echo "    ${BCyan}${faintT}â”Œâ”€â”€${resetT}${BRed}[${DGrey} System ${BRed}]${faintT}${BCyan}â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â•¼${resetT} ${BRed}[${DGrey} ${MySystem} ${BRed}]"

  #--â•¼[ ChecKing ]--â•¼â–° [ System ]--â•¼â–° [ root Access ]
  if [ $(command -v sudo) ]; then
    sudo="sudo"
  else
    sudo=""
  fi

  #--â•¼[ ChecKing ]--â•¼â–° [ System ]--â•¼â–° [ Home Dir ]
  if [ -d $HOME ]; then
    home=$HOME
  else
    home="~/"
  fi
  echo "    ${BCyan}${faintT}|â”€â”€${resetT}${BRed}[${DGrey} Pack Manager ${BRed}]${faintT}${BCyan}â”€â”€â”€â”€â”€â•¼${resetT} ${BRed}[${DGrey} ${MyPackManager} ${BRed}]"
  echo "    ${BCyan}${faintT}|â”€â”€${resetT}${BRed}[${DGrey} Home dir ${BRed}]${faintT}${BCyan}â”€â”€â”€â”€â”€â”€â”€â”€â”€â•¼${resetT} ${BRed}[${DGrey} ${home} ${BRed}]"

  #--â•¼[ ChecKing ]--â•¼â–° [ System ]--â•¼â–° [ Bin Dir ]
  if [ -d /data/data/com.termux/files/usr/bin ]; then
    MyBinDirectory="/data/data/com.termux/files/usr/bin"
  elif [ -d /usr/local/bin ]; then
    MyBinDirectory="/usr/local/bin"
  elif [ -d /bin ]; then
    MyBinDirectory="/bin"
  elif [ -d /sbin ]; then
    MyBinDirectory="/sbin"
  fi
  echo "    ${BCyan}${faintT}â””â”€â”€${resetT}${BRed}[${DGrey} Bin Directory ${BRed}]${faintT}${BCyan}â”€â”€â”€â”€â•¼${resetT} ${BRed}[${DGrey} ${MyBinDirectory} ${BRed}]"
  # cp $0 ${bin}

}
#--â•¼[ ChecKing ]--â•¼â–° [ If ]--â•¼â–° [  Program Exist ]
CheckThatPro(){
  if [ -z $1 ]; then
    echo "\n"
    echo "    ${LRed}|â”€â”€â•¼${BRed}[${slowBlink} âŒ ${resetT}${BRed}]${LRed}â”€â”€â•¼â–° ${BRed}[${LRed} Please Set Something To Find ${BRed}]"
    echo "\n"
  else
    which $1 > /dev/null 2>&1;
    if [ $? = 0 ]; then
      ToolPath=$(which $1)
      echo "    ${BCyan}${faintT}|â”€â”€â•¼${resetT}${BRed}[ âœ… ]${BCyan}${faintT}â”€â”€â•¼${resetT}${LGreen}â•¼â–° ${LCyan}[${ORANGE} ${1} ${LCyan}]${BCyan}${faintT}â”€â”€â•¼${resetT}${LGreen}â•¼â–° ${BRed}[${BCyan} ${ToolPath} ${BRed}]"
    else
      echo "    ${BCyan}${faintT}|â”€â”€â•¼${resetT}${BRed}[ ${slowBlink}âŒ${resetT}${BRed} ]${BCyan}${faintT}â”€â”€â•¼${resetT}${LRed}â•¼â–° ${BWhite}[ ${1} ]${BCyan}${faintT}â”€â”€â•¼${resetT}${LRed}â•¼â–° ${BRed}[ Sorry ${BRed}Not Found! ${BRed}]"
      AwesomeInstallPro ${1}
    fi
  fi
}
