#--â•¼[ ChecKing ]--â•¼â–° [ If ]--â•¼â–° [  Case Folder Exist ]
CheckingifCaseFolderExist(){
  #statements
  if [ -d Case ]; then
    echo "\n          ${BBlue}|â”€â”€â”€â”€${BRed}[ âœ… ${BRed}]${BBlue}â”€â”€â•¼ ${BGreen}Case ${BYellow}Folder Exists, Continue to it\n"
    cd Case
    if [ -d $targetNAME ]; then
      #statements
      echo "          ${BBlue}|â”€â”€â”€â”€${BRed}[ âœ… ${BRed}]${BBlue}â”€â”€â•¼ ${BGreen}$targetNAME ${BYellow}Folder Exists, Continue to it\n"
      cd ${targetNAME}
    else
      echo "\n          ${BBlue}|â”€â”€â”€â”€${BRed}[ âŒ ${BRed}]${BBlue}â”€â”€â•¼ ${BGreen}${targetNAME} ${BYellow}Folder Not Exists, Creating it\n"
      mkdir ${targetNAME}
      cd ${targetNAME}
    fi

    if [ -d Planning ]; then
      #statements
      echo "          ${BBlue}|â”€â”€â”€â”€${BRed}[ âœ… ${BRed}]${BBlue}â”€â”€â•¼ ${BGreen}$targetNAME/Planning ${BYellow}Folder Exists, Continue to it\n"
      cd Planning
    else
      echo "\n          ${BBlue}|â”€â”€â”€â”€${BRed}[ âŒ ${BRed}]${BBlue}â”€â”€â•¼ ${BGreen}Planning ${BYellow}Folder Not Exists, Creating it\n"
      mkdir Planning
      cd Planning
    fi
  else
    echo "\n          ${BBlue}|â”€â”€â”€â”€${BRed}[ âŒ ${BRed}]${BBlue}â”€â”€â•¼ ${BGreen}Case ${BYellow}Folder Not Exists, Creating it\n"
    echo "          ${BBlue}|â”€â”€â”€â”€${BRed}[ âœ… ${BRed}]${BBlue}â”€â”€â•¼ ${BYellow}Creating ${BGreen}Case ${BYellow}Folder and Continue to it\n"
    mkdir Case
    cd Case
    echo "          ${BBlue}|â”€â”€â”€â”€${BRed}[ âœ… ${BRed}]${BBlue}â”€â”€â•¼ ${BYellow}Creating ${BGreen}$targetNAME ${BYellow}and Continue to it\n"
    mkdir $targetNAME
    cd $targetNAME
    echo "          ${BBlue}|â”€â”€â”€â”€${BRed}[ âœ… ${BRed}]${BBlue}â”€â”€â•¼ ${BYellow}Creating ${BGreen}Case Planning ${BYellow}and Continue to it\n"
    mkdir Planning
    cd Planning
  fi

  if [ -e ${Planning_File} ]; then
    t5="$(echo "          ${BBlue}|â”€â”€â”€â”€${BRed}[ âŒ ${BRed}]${BBlue}â”€â”€â•¼${BGreen} $Planning_File ${BGreen} ${BYellow}Exist! ${LRed}Remove it? ${BCyan}[yY/nN]: ")"
    read -p "$t5 " filexist
    case $filexist in
      y|Y)
      cd "$MainDirectoriyCaseName/Planning"
      echo "          ${BBlue}|â”€â”€â”€â”€${BRed}[ ğŸ”¶ ${BRed}]${BBlue}â”€â”€â•¼${BGreen}  Removing {${Planning_File}} file .."
      Press_ENTER
      rm ${Planning_File}
      echo "          ${BBlue}|â”€â”€â”€â”€${BRed}[ âŒ ${BRed}]${BBlue}â”€â”€â•¼${BGreen} âœ… Removed {${Planning_File}} file .."
      cd "$MainDirectoriyCaseName"
      ;;
      n|N)
      echo "          ${BBlue}|â”€â”€â”€â”€${BRed}[ ğŸ”¶ ${BRed}]${BBlue}â”€â”€â•¼${BGreen} Leaving {${Planning_File}} file .."
      ;;
    esac
  fi

}
#--â•¼[ ChecKing ]--â•¼â–° [ If ]--â•¼â–° [ Internet Connection Exist ]
CheckInternetConnection(){
  ping -c 1 google.com > /dev/null 2>&1
  if [ "$?" == 0 ]; then
    InternetStatus="\n    ${DGrey}[âœ…${DGrey}]${BYellow}${faintT}â”€â”€â•¼${resetT}${LGreen}â–°â–°â–°  ${resetT}${DGrey}[${LGrey} Internet ${DGrey}]${BYellow}${faintT}â”€â”€â”€â”€â”€â”€â”€â•¼${resetT}${LGreen}â–°â–°â–° ${DGrey}[${BGreen}${slowBlink} True ${resetT}${DGrey}] \n"
    echo "${InternetStatus}"
    if [ ! -z "$targetDns" ]; then
      #statements
      GetIP="$(ping -c 1 "${targetDns}" | grep ttl | cut -d " " -f4 |  tr  -d :)"
      targetIP="${GetIP}"
    fi
    clear

  else
    InternetStatus="\n    ${DGrey}[âŒ${DGrey}]${BYellow}${faintT}â”€â”€â•¼${resetT}${LRed}â–°â–°â–°  ${resetT}${DGrey}[${LGrey} Internet ${DGrey}]${BYellow}${faintT}â”€â”€â”€â”€â”€â”€â”€â•¼${resetT}${LRed}â–°â–°â–° ${DGrey}[${BRed}${slowBlink} False ${resetT}${DGrey}] ${BRed}\n"
    echo "${InternetStatus}"
  fi
}
#--â•¼[ ChecKing ]--â•¼â–° [ If ]--â•¼â–° [ User is Root ]
CheckingIfRoot(){
  ROOT_UID=0
  if [ "$UID" -eq "$ROOT_UID" ]; then
    echo "Yes, you are root."
  else
    echo "No, you are not root"
  fi
}
