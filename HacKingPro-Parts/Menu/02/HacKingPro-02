#--╼▰[ Main Menu ]--╼▰ [ 02 ]--╼▰ [  Weaponization  ]
HacKingPro_Weaponization(){
  while true; do
    cd "$x_Menu_2"
    clear
    ToolHeaderRun
    TargetStatusRun
    HacKingProMenuMain=$(echo "
    ${DGrey}┌────╼ ${BRed}[${BYellow} Weaponization ${BRed}]
    ${DGrey}┖─┲──╼ ${BRed}[${BYellow} Intruder develops malware designed to exploit the vulnerability ${BRed}]
    ${DGrey}  ┖──╼ ${BRed}[${BYellow} Intruder creates remote access malware weapon, such as a virus or worm, tailored to one or more vulnerabilities. ${BRed}]
    ${DGrey}
    ${DGrey}┌──────╼ ${BRed}[${BYellow} Weaponization ${BRed}]
    ${DGrey}|
    ${DGrey}|──${BRed}[${LYellow} 01 ${BRed}]${DGrey}───╼ ${BRed}[${BYellow} Windows ${BRed}]
    ${DGrey}|
    ${DGrey}|──${BRed}[${LYellow} 02 ${BRed}]${DGrey}───╼ ${BRed}[${BYellow} Android ${BRed}]
    ${DGrey}|
    ${DGrey}|──${BRed}[${LYellow} 03 ${BRed}]${DGrey}───╼ ${BRed}[${BYellow} MacOS ${BRed}]
    ${DGrey}|
    ${DGrey}|──${BRed}[${LYellow} 04 ${BRed}]${DGrey}───╼ ${BRed}[${BYellow} Linux ${BRed}]
    ${DGrey}|
    ${DGrey}└──${BRed}[${DGrey} X ${BRed}]${BGreen} ╼ ${BRed}[${DGrey} Exit / Quit / Close ${BRed}]


    ${BGreen}┌──${BRed}[${BCyan} Arsenal ${BRed}]${BYellow}╼▰  ${BRed}[${BCyan} Cheat Sheets ${BRed}]${BYellow}╼▰  ${BRed}[${BGreen} Command ${BRed}]${BYellow}╼▰  ${BRed}[${LGreen} $ArsenalCommand ${BRed}]
    ${BGreen}|
    ${BGreen}|──${BRed}[${DGrey} ~$(pwd) ${BRed}]
    ${BGreen}|
    ${BGreen}└──${BRed}[${BYellow} $(AnLoMinus) ${BRed}]${BYellow}╼▰  ${BRed}[${BGreen} ${HacKingPro} ${BRed}]${BYellow}╼▰  ${BRed}[${BGreen} Weaponization ${BRed}]${BYellow}╼▰ ")
    read -p "$HacKingProMenuMain" cmd
    case $cmd in
      1|01|windows|Windows )
      WindowsWeaponizationPro
      Press_ENTER
      ;;
      ls )
      lahs
      ;;
      2|02|android|Android )
      AndroidPlan
      Press_ENTER
      ;;
      4|04 )
      MainDirectoriyCaseNameBrutePort="${MainDirectoriyCaseName}/BrutePort/"
      mkdir "${MainDirectoriyCaseNameBrutePort}"
      cd "${MainDirectoriyCaseNameBrutePort}"
      echo "$(BrutePort)" >> ${targetNAME}-HacKingPro-BrutePort.log
      less -R "${targetNAME}-HacKingPro-BrutePort.log"
      Press_ENTER
      ;;
      12 )
      PrivilegEscalationPro
      Press_ENTER
      ;;
      22|"Kerberos" )
      AttacKingKerberos
      Press_ENTER
      ;;
      arsenal|Arsenal|cs|CS|cheats|Cheatsheets|CheatSheets|Commands|commands|zz )
      Arsenal
      ;;
      p|P )
      HacKingProPlanning
      ReportPlanning
      ;;
      x|X|exit|EXIT )
      cd ${MainDirectoriy}
      clear
      AwesomeLineExit
      break
      ;;
    esac
  done
}
