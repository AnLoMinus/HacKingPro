#--╼▰[ Main Menu ]--╼▰ [ 02 ]--╼▰ [  Weaponization  ]--╼▰ [  Windows Weapons  ]
WindowsWeaponizationPro(){
  while true; do
  clear
  ToolHeaderRun
  TargetStatusRun
  WindowsWeaponizationProMenuMain=$(echo "
  ${DGrey}┌────╼ ${BRed}[${BYellow} Windows Weaponization ${BRed}]
  ${DGrey}┖─┲──╼ ${BRed}[${BYellow} xxxxxx ${BRed}]
  ${DGrey}  ┖──╼ ${BRed}[${BYellow} xxxxxx ${BRed}]
  ${DGrey}
  ${DGrey}┌──────╼ ${BRed}[${BYellow} Weapons ${BRed}]
  ${DGrey}|
  ${DGrey}|──${BRed}[${LYellow} 01 ${BRed}]${DGrey}───╼ ${BRed}[${BYellow} AttacKing Kerberos ${BRed}]
  ${DGrey}|
  ${DGrey}└──${BRed}[${DGrey} X ${BRed}]${BGreen} ╼ ${BRed}[${DGrey} Exit / Quit / Close ${BRed}]


  ${BGreen}┌──${BRed}[${BCyan} Arsenal ${BRed}]${BYellow}╼▰  ${BRed}[${BCyan} Cheat Sheets ${BRed}]${BYellow}╼▰  ${BRed}[${BGreen} Command ${BRed}]${BYellow}╼▰  ${BRed}[${LGreen} $ArsenalCommand ${BRed}]
  ${BGreen}|
  ${BGreen}|──${BRed}[${DGrey} ~$(pwd) ${BRed}]
  ${BGreen}|
  ${BGreen}└──${BRed}[${BYellow} $(AnLoMinus) ${BRed}]${BYellow}╼▰  ${BRed}[${BGreen} ${HacKingPro} ${BRed}]${BYellow}╼▰  ${BRed}[${BGreen} Weaponization ${BRed}]${BYellow}╼▰ ")
  read -p "$WindowsWeaponizationProMenuMain" cmd
  case $cmd in
    1|01|ak|AK|"Kerberos" )
    AttacKingKerberos
    Press_ENTER
    ;;
    ls )
    lahs
    ;;
    x|X|exit|EXIT )
    cd ${MainDirectoriy}
    clear
    AwesomeLineExit
    break
    ;;
  esac
done

}
