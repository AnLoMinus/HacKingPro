Host_Info(){
   cd "${MainDirectoriyCaseNameLocal}"
	 dnsips=$(sed -e '/^$/d' /etc/resolv.conf | awk '{if (tolower($1)=="nameserver") print $2}')

   echo "
    ${BCyan}${faintT}-=x=-=x=-=x=-=x=-=x=-=x=-=[ Hostname and DNS information ]=-=x=-=x=-=x=-=x=-=x=-=x=-=x=-=x=-

    ${DGrey}â”Œâ”€â”€â”€â”€â”€â”€â”€â•¼ ${BRed}[${BYellow} Hostname and DNS information ${BRed}]
    ${DGrey}|
    ${DGrey}|â”€â”€â•¼ ${BRed}[${ORANGE} Hostname : ${resetT}${ORANGE}${faintT}$(hostname)
    ${DGrey}|â”€â”€â•¼ ${BRed}[${ORANGE} DNS domain : ${resetT}${ORANGE}${faintT}$(hostname -d)
    ${DGrey}|â”€â”€â•¼ ${BRed}[${ORANGE} Fully qualified domain name : ${resetT}${ORANGE}${faintT}$(hostname -f)
    ${DGrey}|â”€â”€â•¼ ${BRed}[${ORANGE} Network address (IP) :  ${resetT}${ORANGE}${faintT}$(hostname -i)
    ${DGrey}â”–â”€â”€â•¼ ${BRed}[${ORANGE} DNS name servers (DNS IP) ${resetT}${ORANGE}${faintT}: ${dnsips}

    ${BCyan}${faintT}-=x=-=x=-=x=-=x=-=x=-=x=-=[ Hostname and DNS information ]=-=x=-=x=-=x=-=x=-=x=-=x=-=x=-=x=-
    "
}
#--â•¼â–°[ Main Menu ]--â•¼â–° [ 01 ]--â•¼â–° [  Reconnaissance & Information Gathering  ]--â•¼â–° [ CLI ]--â•¼â–° [ Get Bacsic Information About OS ]
BasicInfo() {
  # Checking Basic Information
  knam=$(uname -s)    2> /dev/null # Kernel name
  krel=$(uname -r)    2> /dev/null # Kernel release
  kver=$(uname -v)    2> /dev/null # Kernel version
  arch=$(uname -m)    2> /dev/null # Architecture
  echo "
  ${BCyan}${faintT}-=x=-=x=-=x=-=x=-=x=-=x=-=[ Basic Information ]=-=x=-=x=-=x=-=x=-=x=-=x=-=x=-=x=-

  ${DGrey}â”Œâ”€â”€â”€â”€â”€â”€â”€â•¼ ${BRed}[${BYellow} System Information ${BRed}]
  ${DGrey}|
  ${DGrey}|â”€â”€â•¼ ${BRed}[${BYellow} Script ID:      ${BRed}]${BCyan}${faintT}â”€â”€â”€â•¼ ${resetT}${BRed}[${ORANGE}${nc} $$ ${BRed}]
  ${DGrey}|â”€â”€â•¼ ${BRed}[${BYellow} Kernel Name     ${BRed}]${BCyan}${faintT}â”€â”€â”€â•¼ ${resetT}${BRed}[${ORANGE}${nc} ${knam} ${BRed}]
  ${DGrey}|â”€â”€â•¼ ${BRed}[${BYellow} Kernel Release  ${BRed}]${BCyan}${faintT}â”€â”€â”€â•¼ ${resetT}${BRed}[${ORANGE}${nc} ${krel} ${BRed}]
  ${DGrey}|â”€â”€â•¼ ${BRed}[${BYellow} kernel Version  ${BRed}]${BCyan}${faintT}â”€â”€â”€â•¼ ${resetT}${BRed}[${ORANGE}${nc} ${kver} ${BRed}]
  ${DGrey}|â”€â”€â•¼ ${BRed}[${BYellow} Architecture    ${BRed}]${BCyan}${faintT}â”€â”€â”€â•¼ ${resetT}${BRed}[${ORANGE}${nc} ${arch} ${BRed}]
  ${DGrey}|â”€â”€â•¼ ${BRed}[${BYellow} Term            ${BRed}]${BCyan}${faintT}â”€â”€â”€â•¼ ${resetT}${BRed}[${ORANGE}${nc} ${TERM} ${BRed}]
  ${DGrey}|â”€â”€â•¼ ${BRed}[${BYellow} Shell           ${BRed}]${BCyan}${faintT}â”€â”€â”€â•¼ ${resetT}${BRed}[${ORANGE}${nc} $SHELL ${BRed}]
  ${DGrey}|â”€â”€â•¼ ${BRed}[${BYellow} Date            ${BRed}]${BCyan}${faintT}â”€â”€â”€â•¼ ${resetT}${BRed}[${ORANGE}${nc} $(date) ${BRed}]
  ${DGrey}|â”€â”€â•¼ ${BRed}[${BYellow} UpTime          ${BRed}]${BCyan}${faintT}â”€â”€â”€â•¼ ${resetT}${BRed}[${ORANGE}${nc} $(uptime) ${BRed}]
  ${DGrey}|â”€â”€â•¼ ${BRed}[${BYellow} OS Name         ${BRed}]${BCyan}${faintT}â”€â”€â”€â•¼ ${resetT}${BRed}[${ORANGE}${nc} ${osys} ${BRed}]
  ${DGrey}|â”€â”€â•¼ ${BRed}[${BYellow} Path            ${BRed}]${BCyan}${faintT}â”€â”€â”€â•¼ ${resetT}${BRed}[${ORANGE}${nc} \n$PATH ${BRed}]
  ${DGrey}â”–â”€â”€â•¼ ${BRed}[${BYellow} who is currently logged in:${LYellow} \n$(who)

  ${BCyan}${faintT}-=x=-=x=-=x=-=x=-=x=-=x=-=[ Basic Information ]=-=x=-=x=-=x=-=x=-=x=-=x=-=x=-=x=-"

}
