    read -p "$HacKingProMenuMain" cmd
    case $cmd in
      lpp|LPP )
      File_LinPrivPro="${MainDirectoriyCaseNameLocal}/${targetNAME}-HacKingPro-LinPrivPro-Log.md"  # Define Recon File
      LinPrivPro >> ${File_LinPrivPro}
      less -R ${File_LinPrivPro}
      Press_ENTER
      ;;
      lpe|LPE )
      LinPrivEsc
      Press_ENTER
      ;;
      "?"|help|about|About )
