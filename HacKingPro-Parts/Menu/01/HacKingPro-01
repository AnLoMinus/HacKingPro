    printf "    ${BWhite}[${resetT}${BGreen}+${resetT}${BWhite}] Spotify: ${resetT}"
    check1=$(curl -s -i "https://open.spotify.com/user/$targetNAME" -H "Accept-Language: en" -L | grep -o 'HTTP/2 404' ; echo $?)

    if [[ $check1 == *'0'* ]] ; then
