    # cat /var/log/cups/error_log
    # cat /var/log/dpkg.log
    # cat /var/log/faillog
    # cat /var/log/httpd/access_log
    # cat /var/log/httpd/access.log
    # cat /var/log/httpd/error_log
    # cat /var/log/httpd/error.log
    # cat /var/log/lastlog
