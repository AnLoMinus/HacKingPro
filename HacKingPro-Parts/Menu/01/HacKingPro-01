    elif [[ $check1 == *'1'* ]]; then
      printf "${BGreen} Found!${resetT} https://%s.deviantart.com\n" $targetNAME
      printf "https://%s.deviantart.com\n" $targetNAME >> ${targetNAME}-UserFinder.txt
    fi

