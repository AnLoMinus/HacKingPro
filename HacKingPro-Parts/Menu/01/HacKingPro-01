    printf "    ${BWhite}[${resetT}${BGreen}+${resetT}${BWhite}] DeviantART: ${resetT}"
    check1=$(curl -s -i "https://$targetNAME.deviantart.com" -H "Accept-Language: en" -L | grep -o 'HTTP/2 404' ; echo $?)
    if [[ $check1 == *'0'* ]] ; then
