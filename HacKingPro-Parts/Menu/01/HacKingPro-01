    elif [[ $check1 == *'1'* ]]; then
      printf "${BGreen} Found!${resetT} https://open.spotify.com/user/%s\n" $targetNAME
      printf "https://open.spotify.com/user/%s\n" $targetNAME >> ${targetNAME}-UserFinder.txt
