          # ${BBlue}â”Œâ”€â”€[${BRed} 00 ${BBlue}]â”€â”€â•¼ ${BBlue}[${BRed} Orusula ${BBlue}]${BRed}â”€â”€â•¼ ${BBlue}[${LRed} âŒ ${BBlue}]$BGreen
          # ${BBlue}|
          # ${BBlue}|â”€â”€[${BRed} 01 ${BBlue}]â”€â”€â•¼ ${BBlue}[${BRed} Brute Force Ports ${BBlue}]${BRed}â”€â”€â•¼ ${BBlue}[${LRed} âœ… ${BBlue}]$BGreen
          # ${BBlue}|
          # ${BBlue}â””â”€â”€[${BRed} 03 ${BBlue}]â”€â”€â•¼ ${BBlue}[${BRed} Orusula ${BBlue}]${BRed}â”€â”€â•¼ ${BBlue}[${LRed} âŒ ${BBlue}]$BGreen

    read -p "$HacKingProMenuMain" cmd
