      ${BCyan}            â”Œâ”€â”€${BBlue}[${BBlue} Anlominus ğŸ‘½${BRed} KoTH ${BCyan}$~${BBlue}]
      ${BCyan}            â””â”€â”€â•¼ ")
          read -p "$KoTHMenuMain" cmd
          case $cmd in
            lpe|LPE )
            cd $xMenu14
            cd Linux/Enumeration
            sleep 1
            if [ -e linpeas_base.sh ]; then
              #statements
              echo "\n${BRed}[${LGrey}LPE${BRed}]${BBlue} -${DGrey} âœ… Found ${BGreen}linpeas_base.sh\n${BBlue}"
            else
              echo "\n${BRed}[${LGrey}LPE${BRed}]${BBlue} -${DGrey} ğŸ”¶ Downloading ${BGreen}linpeas_base.sh\n${BBlue}"
              wget https://raw.githubusercontent.com/Anlominus/HacKingPro/main/Menu/14--Privilege%20Enumeration%20%26%20Escalation/Linux/Enumeration/linpeas_base.sh
            fi
            if [ -e linuxallenum.sh ]; then
              #statements
              echo "\n${BRed}[${LGrey}LPE${BRed}]${BBlue} -${DGrey} âœ… Found ${BGreen}linuxallenum.sh\n${BBlue}"
            else
              echo "\n${BRed}[${LGrey}LPE${BRed}]${BBlue} -${DGrey} ğŸ”¶ Downloading ${BGreen}linuxallenum.sh\n${BBlue}"
              wget https://raw.githubusercontent.com/Anlominus/HacKingPro/main/Menu/14--Privilege%20Enumeration%20%26%20Escalation/Linux/Enumeration/linuxallenum.sh
            fi
            if [ -e lse.sh ]; then
              #statements
              echo "\n${BRed}[${LGrey}LPE${BRed}]${BBlue} -${DGrey} âœ… Found ${BGreen}lse.sh\n${BBlue}"
            else
              echo "\n${BRed}[${LGrey}LPE${BRed}]${BBlue} -${DGrey} ğŸ”¶ Downloading ${BGreen}lse.sh\n${BBlue}"
              wget https://raw.githubusercontent.com/Anlominus/HacKingPro/main/Menu/14--Privilege%20Enumeration%20%26%20Escalation/Linux/Enumeration/lse.sh
            fi
            sleep 1
            echo "\n${BRed}[${LGrey}LPE${BRed}]${BBlue} -${DGrey} âœ… ${BGreen}All Downloaded Linux-PrivEsc GiTools\n"
