# Read Target Name From the User
ReadTargetNameFromtheUser(){
  # Read Target Name From the User
  # This Variable is for Case Name.
  t0="$(echo "\n          ${BBlue}â”Œâ”€â”€[ ğŸ¯ ]â”€â”€[${BRed} Name ${BBlue}]â”€â”€â•¼  ")"
  read -p "$t0" targetNAME
  if [ -z "$targetNAME" ]; then
    #statements
