            echo " "
            read -p " Press Enter" some
            ;;
            wpe|WPE )
            echo "${BRed}[${LGrey}WPE${BRed}]${BBlue} -${DGrey} Windows-PrivEsc"
            read_ifconfig="$(ifconfig)"
            echo "$read_ifconfig"
            sleep 3
            echo " "
            # sleep 1
            # git clone https://github.com/Anlominus/Windows-PrivEsc
            # sleep 1
