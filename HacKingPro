    ${DGrey}|
    ${DGrey}â””â”€â”€${BRed}[${DGrey} X ${BRed}]${BGreen} â•¼ ${BRed}[${DGrey} Exit / Quit / Close ${BRed}]


    ${BBlue}â”Œâ”€â”€${BBlue}[${BRed} Arsenal ${BRed}]${BGreen}â”€â”€â•¼ ${BRed}[${BGreen} Cheat Sheets ${BRed}]${BGreen}â”€â”€â•¼ ${BRed}[${BGreen} Command ${BRed}]${BGreen}â”€â”€â•¼ ${BRed}[${LGreen} $ArsenalCommand ${BRed}]
    ${BBlue}|
    ${DGrey}|â”€â”€[ ~$(pwd) ]
    ${DGrey}|
    ${DGrey}â””â”€â”€${BRed}[${BYellow} $Anlominus ${BRed}]${BGreen}â”€â”€â•¼ ${BRed}[${BGreen} ${HacKingPro} ${BRed}]${BGreen}â”€â”€â•¼ ${BRed}[${BYellow} LockHeed ${BRed}]${BGreen}â”€â”€â•¼ ${BRed}[${BGreen} ")

    read -p "$HacKingProMenuMain" cmd
    case $cmd in
      ls )
      ls_lahs="$(echo "\n\t ${BRed}[ğŸ™ğŸ¼${BRed}] ${BWhite}List Files ~ Executing: ls -lahs ${HacKingPro} ${BRed}[ğŸ™ğŸ¼${BRed}]\n\t\t ${HacKingPro}")"
      echo "$ls_lahs"
      sleep 2
      ls -lahs
      sleep 2
      read -p "$ls_lahs" cmd
      ;;
      arsenal|Arsenal|cs|CS|cheats|Cheatsheets|CheatSheets|Commands|commands|zz )
      Arsenal
      ;;
      p|P )
      HacKingProPlanning
      ;;
      x|X|exit|EXIT )
      cd $XHacKingPro
      clear
      break
      ;;
    esac
  done
    ;;
  esac
done
}

