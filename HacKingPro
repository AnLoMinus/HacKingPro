            # echo "\n\t\t${BRed}[${BYellow}ğŸ”¶${BRed}]${BBlue} - ${BYellow}Creating $targetNAME and Continue to it\n"
            echo "\n\t\t${BRed}[${BYellow}ğŸ”¶${BRed}]${BBlue} - ${BYellow}Creating Reconnaissance and Continue to it\n"
