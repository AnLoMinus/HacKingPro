    ${BGreen}|
    ${BGreen}|â”€â”€${BRed}[${DGrey} ~$(pwd) ${BRed}]
    ${BGreen}|
    ${BGreen}â””â”€â”€${BRed}[${BYellow} $Anlominus ${BRed}]${BGreen}â”€â”€â•¼ ${BRed}[${BGreen} ${HacKingPro} ${BRed}]${BGreen}â”€â”€â•¼ ${BRed}[${BGreen} Weaponization ${BRed}]${BGreen}â”€â”€â•¼ ${BGreen}   ")
            read -p "$HacKingProMenuMain" cmd
            case $cmd in
              0|00 )
              #
              xMenu02_arp=$(arp -an)
              xMenu02_arp_x=$(
              echo "\n\t\t${BGreen}### Arp Scan Log Time: $(date)"
              echo "# Arp Scan Log $targetIP: " >> ${targetNAME}-HacKingPro-Reconnaissance-Log.md
