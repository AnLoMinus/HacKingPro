      echo "# [2] - Reconnaissance & Vulnerability Assessment " >> ${targetNAME}-HacKingPro-Recon-Log.md
      echo " " >> ${targetNAME}-HacKingPro-Recon-Log.md
      #
      xMenu02_arp=$(arp -an)
      xMenu02_arp_x=$(
