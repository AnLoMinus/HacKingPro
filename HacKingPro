    echo "    ${BGreen}|â”€â”€${BRed}[ P ]${BGreen}â”€â”€â•¼ ${BRed}[ âš¡ï¸ Planning and Scoping ğŸ¯ ]"
    sleep 1
    echo "    ${BGreen}|â”€â”€${BRed}[ P ]${BGreen}â”€â”€â•¼ ${BRed}[ âš¡ï¸ Planning and Scoping ğŸ¯ ]"
    sleep 1
    echo "    ${BGreen}|â”€â”€${BRed}[ P ]${BGreen}â”€â”€â•¼ ${BRed}[ âš¡ï¸ Planning and Scoping ğŸ¯ ]"
    sleep 1
    echo "    ${BGreen}â””â”€â”€${BRed}[ P ]${BGreen}â”€â”€â•¼ ${BRed}[ âš¡ï¸ Planning and Scoping ğŸ¯ ]"
    sleep 1
    read -p "$Press_ENTER" readthat
    cd "$XHacKingPro"
  fi

}

###############################################

# Parsing Date and Time
year=$(date +%Y)
month=$(date +%m)
day=$(date +%d)
hour=$(date +%H)
minute=$(date +%M)
second=$(date +%S)
CurrentDate=$(echo "${DGrey}$day-$month-$year")
StartingTime=$(echo "${DGrey}$hour:$minute:$second")
FullTime=$(date)

# HacKingPro Design
HacKingPro=$(echo "${BRed}Hac${BYellow}King${BBlue}Pro")
Anlominus=$(echo "${BGreen}An${DGrey}Lo${BRed}Minus")
MyVersion=$(echo "${DGrey}V0.0.0.${BWhite}15")
HacKingProDavid=$(echo "\n\t  ${BRed}[${BCyan}âœ¡${BRed}] ${HacKingPro} ${BRed}[${BCyan}âœ¡${BRed}]\n\t\t ${BGreen}")
HacKingProExit=$(echo "\n ${BRed}[${BCyan}âœ¡${BRed}] ${BCyan}Exit ${HacKingPro} ${BRed}[${BCyan}âœ¡${BRed}]\n\t\t ${BGreen}")
Press_ENTER=$(echo "\n\n${BYellow}                ğŸ”¶    Press ENTER    ğŸ”¶     ${BGreen}\n\n")
Press_Enter_To_START=$(echo "\n\n${BYellow}                ğŸ”¶    Press Enter To START    ğŸ”¶     ${BGreen}\n\n")
InvalidOption=$(echo "\n\t\t ${BGreen}Â»Â»Â» ${HacKingPro} ${LRed}Invalid Option ${BRed}Â¯\_(ãƒ„)_/Â¯ \n")
###############################################

