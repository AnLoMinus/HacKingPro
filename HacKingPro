# Checking if Case Folder Exist
CheckingifCaseFolderExist(){
  ReadTargetNameFromtheUser
  #statements
  if [ -d Case ]; then
    echo "\n          ${BBlue}|â”€â”€â”€â”€${BRed}[ âœ… ${BRed}]${BBlue}â”€â”€â•¼ ${BGreen}Case ${BYellow}Folder Exists, Continue to it\n"
    cd Case
    if [ -d $targetNAME ]; then
      #statements
      echo "          ${BBlue}|â”€â”€â”€â”€${BRed}[ âœ… ${BRed}]${BBlue}â”€â”€â•¼ ${BGreen}$targetNAME ${BYellow}Folder Exists, Continue to it\n"
      cd $targetNAME
    else
      mkdir $targetNAME
      cd $targetNAME
    fi
    if [ -d Planning ]; then
      #statements
      echo "          ${BBlue}|â”€â”€â”€â”€${BRed}[ âœ… ${BRed}]${BBlue}â”€â”€â•¼ ${BGreen}$targetNAME/Planning ${BYellow}Folder Exists, Continue to it\n"
      cd Planning
    else
      mkdir Planning
      cd Planning
    fi
  else
    echo "\n          ${BBlue}|â”€â”€â”€â”€${BRed}[ âŒ ${BRed}]${BBlue}â”€â”€â•¼ ${BGreen}Case ${BYellow}Folder Not Exists, Create it\n"
    echo "          ${BBlue}|â”€â”€â”€â”€${BRed}[ âœ… ${BRed}]${BBlue}â”€â”€â•¼ ${BYellow}Creating ${BGreen}Case ${BYellow}Folder and Continue to it\n"
    mkdir Case
    cd Case
    echo "          ${BBlue}|â”€â”€â”€â”€${BRed}[ âœ… ${BRed}]${BBlue}â”€â”€â•¼ ${BYellow}Creating ${BGreen}$targetNAME ${BYellow}and Continue to it\n"
    mkdir $targetNAME
    cd $targetNAME
    echo "          ${BBlue}|â”€â”€â”€â”€${BRed}[ âœ… ${BRed}]${BBlue}â”€â”€â•¼ ${BYellow}Creating ${BGreen}Case Planning ${BYellow}and Continue to it\n"
    mkdir Planning
    cd Planning
  fi

  # This Variable for Naming File Report
  Planning_File="${targetNAME}-HacKingPro-Planning-Log.md"
  if [ -e ${Planning_File} ]; then
    t5="$(echo "          ${BBlue}|â”€â”€â”€â”€${BRed}[ âŒ ${BRed}]${BBlue}â”€â”€â•¼${BGreen} $Planning_File ${BGreen} ${BYellow}Exist! ${LRed}Remove it? ${BCyan}[yY/nN]: ")"
    read -p "$t5 " filexist
    case $filexist in
      y|Y)
      cd "$XHacKingProCaseTargetName/Planning"
      echo "          ${BBlue}|â”€â”€â”€â”€${BRed}[ ğŸ”¶ ${BRed}]${BBlue}â”€â”€â•¼${BGreen}  Removing {${Planning_File}} file .."
      read -p "$Press_ENTER"
      rm ${Planning_File}
      echo "          ${BBlue}|â”€â”€â”€â”€${BRed}[ âŒ ${BRed}]${BBlue}â”€â”€â•¼${BGreen} âœ… Removed {${Planning_File}} file .."
      cd "$XHacKingProCaseTargetName"
      ;;
      n|N)
      echo "          ${BBlue}|â”€â”€â”€â”€${BRed}[ ğŸ”¶ ${BRed}]${BBlue}â”€â”€â•¼${BGreen} Leaving {${Planning_File}} file .."
      ;;
    esac
  fi

}

# Checking For System Package Manager
Check_System() {
  # checking for system package manager
  if [ -e /data/data/com.termux/files/usr/bin/pkg ]; then
  pac="pkg"
  system="termux"
elif [ $(command -v brew) ]; then
    pac="brew"
    system="mac"
    sudo=""
  elif [ $(command -v apt) ]; then
    pac="apt"
    system="linux"
  elif [ $(command -v apt-get) ]; then
    pac="apt-get"
    system="linux"
  elif [ $(command -v apk) ]; then
    pac="apk"
    system="linux"
  elif [ $(command -v yum) ]; then
    pac="yum"
    system="fedora"
  fi

  # checking for system root access
  if [ $(command -v sudo) ]; then
    sudo="sudo"
  else
    sudo=""
  fi

  # checking for system home dir
  if [ -d $HOME ]; then
    home=$HOME
  else
    home="~/"
  fi

  # checking for system bin dir
  if [ -d /data/data/com.termux/files/usr/bin ]; then
    bin="/data/data/com.termux/files/usr/bin"
  elif [ -d /usr/local/bin ]; then
    bin="/usr/local/bin"
  elif [ -d /bin ]; then
    bin="/bin"
  elif [ -d /sbin ]; then
    bin="/sbin"
  fi

}
Check_System

# Checking If User is Root
CheckingIfRoot(){
  ROOT_UID=0
  if [ "$UID" -eq "$ROOT_UID" ]; then
    echo "Yes, you are root."
  else
    echo "No, you are not root"
  fi
}

