# Cover Me Cleaning Logs and more.
CoverMe(){

  LOGS_FILES=(
  ~/.zsh_sessions/*
  /var/log/messages # General message and system related stuff
  /var/log/auth.log # Authenication logs
  /var/log/kern.log # Kernel logs
  /var/log/cron.log # Crond logs
  /var/log/maillog # Mail server logs
  /var/log/boot.log # System boot log
  /var/log/mysqld.log # MySQL database server log file
  /var/log/qmail # Qmail log directory
  /var/log/httpd # Apache access and error logs directory
  /var/log/lighttpd # Lighttpd access and error logs directory
  /var/log/secure # Authentication log
  /var/log/utmp # Login records file
  /var/log/wtmp # Login records file
  /var/log/yum.log # Yum command log file
  /var/log/system.log # System Log
  /var/log/DiagnosticMessages # Mac Analytics Data
  /Library/Logs # System Application Logs
  /Library/Logs/DiagnosticReports # System Reports
  ~/Library/Logs # User Application Logs
  ~/Library/Logs/DiagnosticReports # User Reports
  )

  # CoverMyAss Cleaning Logs

    MainMenu () {
      isRoot () {
              if [ "$EUID" -ne 0 ]; then
                      return 1
              fi
      }

      disableAuth () {
        if [ -w /var/log/auth.log ]; then
          ln /dev/null /var/log/auth.log -sf
          echo " "
          echo "[+] Permanently sending /var/log/auth.log to /dev/null"
          read -p "$Press_ENTER"
        else
          echo " "
          echo "[!] /var/log/auth.log is not writable! Retry using sudo."
          read -p "$Press_ENTER"
        fi
      }

      disableHistory () {
        ln /dev/null ~/.bash_history -sf
        echo " "
        echo "[+] Permanently sending bash_history to /dev/null"
        read -p "$Press_ENTER"

        if [ -f ~/.zsh_history ]; then
          ln /dev/null ~/.zsh_history -sf
          echo " "
          echo "[+] Permanently sending zsh_history to /dev/null"
          read -p "$Press_ENTER"
        fi
        export HISTFILESIZE=0
        export HISTSIZE=0
        echo " "
        echo "[+] Set HISTFILESIZE & HISTSIZE to 0"
        echo " "
        set +o history
        echo "[+] Disabled history library"
        echo "Permenently disabled bash log."
        read -p "$Press_ENTER"
                  }

      enableAuth () {
        if [ -w /var/log/auth.log ] && [ -L /var/log/auth.log ]; then
          rm -rf /var/log/auth.log
          echo " "
          echo " " > /var/log/auth.log
          echo "[+] Disabled sending auth logs to /dev/null"
          read -p "$Press_ENTER"
        else
          echo " "
          echo "[!] /var/log/auth.log is not writable! Retry using sudo."
          read -p "$Press_ENTER"
        fi
      }

      enableHistory () {
        if [ -L ~/.bash_history ]; then
          echo " "
          rm -rf ~/.bash_history
          echo "" > ~/.bash_history
          echo "[+] Disabled sending history to /dev/null"
        fi

        if [ -L ~/.zsh_history ]; then
          echo " "
          rm -rf ~/.zsh_history
          echo "" > ~/.zsh_history
          echo "[+] Disabled sending zsh history to /dev/null"
        fi

        export HISTFILESIZE=" "
        export HISTSIZE=50000
        echo "
      ${BWhite}|
      ${BWhite}|â”€â”€${BRed}[${BGreen} + ${BRed}]${BWhite}â”€â”€â•¼ ${BRed}[${BGreen} Restore HISTFILESIZE & HISTSIZE default values. ${BRed}]
      ${BWhite}|
      ${BWhite}|
      ${BWhite}|â”€â”€${BRed}[${BGreen} + ${BRed}]${BWhite}â”€â”€â•¼ ${BRed}[${BGreen} Enabled history library. ${BRed}]${BGreen}â”€â”€â•¼ ${BRed}[${BGreen} Permenently enabled bash log. ${BRed}]
      ${BWhite}|        "
        echo " "
        set -o history
        echo " "
      }

      clearLogs () {
        for i in "${LOGS_FILES[@]}"
        do
          if [ -f "$i" ]; then
            if [ -w "$i" ]; then
              read -p "$Press_ENTER"
              echo " " > "$i"
              echo " "
              echo "        ${BWhite}|
        ${BWhite}|â”€â”€${BRed}[${BGreen} + ${BRed}]${BWhite}â”€â”€â•¼ ${BRed}[${BGreen} $i Cleaned. ${BRed}]
        ${BWhite}|              "
              read -p "$Press_ENTER"
            else
              echo " "
              echo "
        ${BWhite}|
        ${BWhite}|â”€â”€${BRed}[${BGreen} ! ${BRed}]${BWhite}â”€â”€â•¼ ${BRed}[${BGreen} $i Is Not Writable! Retry Using sudo. ${BRed}]
        ${BWhite}|              "

            fi
          elif [ -d "$i" ]; then
            if [ -w "$i" ]; then
              rm -rf "${i:?}"/*
              echo " "
              echo "
        ${BWhite}|
        ${BWhite}|â”€â”€${BRed}[${BGreen} + ${BRed}]${BWhite}â”€â”€â•¼ ${BRed}[${BGreen} $i Cleaned. ${BRed}]
        ${BWhite}|              "
              read -p "$Press_ENTER"
            else
              echo "
        ${BWhite}|
        ${BWhite}|â”€â”€${BRed}[${BGreen} !! ${BRed}]${BWhite}â”€â”€â•¼ ${BRed}[${BGreen} $i Is Not Writable! Retry Using sudo. ${BRed}]
        ${BWhite}|              "
            fi
          fi
        done
      }

      clearHistory () {
        if [ -f ~/.zsh_history ]; then
          echo " "
          echo " " > ~/.zsh_history
          echo "
        ${BWhite}|
        ${BWhite}|â”€â”€${BRed}[${DGrey} + ${BRed}]${BWhite}â”€â”€â•¼ ${BRed}[${BGreen} ~/.zsh_history Cleaned. ${BRed}]
        ${BWhite}|          "
          read -p "$Press_ENTER"
        fi
        echo " "
        echo " " > ~/.bash_history
        echo "
        ${BWhite}|
        ${BWhite}|â”€â”€${BRed}[${DGrey} + ${BRed}]${BWhite}â”€â”€â•¼ ${BRed}[${BGreen} ~/.bash_history Cleaned. ${BRed}]
        ${BWhite}|        "
        read -p "$Press_ENTER"

        echo " "
        history -c
        read -p "$Press_ENTER"
        echo "
        ${BWhite}|
        ${BWhite}|â”€â”€${BRed}[${DGrey} + ${BRed}]${BWhite}â”€â”€â•¼ ${BRed}[${BGreen}  History File Deleted. ${BRed}]
        ${BWhite}|        "

        echo " "
        echo "
        ${BWhite}|
        ${BWhite}|â”€â”€${BRed}[${BYellow} Reminder ${BRed}]${BWhite}â”€â”€â•¼ ${BRed}[${BYellow} your need to reload the session to see effects. Type exit to do so.${BRed}]
        ${BWhite}|        "
        read -p "$Press_ENTER"
      }

      exitTool () {
              exit 1
      }

      clear # Clear output

      while true; do
        #statements
        clear
        # HacKingProLogo1
        HacKingProTargetStatus=$(echo "
    ${ToolHeader}

    ${DGrey}â”Œâ”€â”€${BRed}[${DGrey} P ${BRed}]${BGreen}â”€â”€â•¼ ${BRed}[${DGrey} âš¡ï¸ Planning and Scoping ğŸ¯ ${BRed}]
    ${DGrey}|
    ${DGrey}|â”€â”€${BRed}[${DGrey} Full Time ${BRed}]${BGreen}â”€â”€â•¼ ${BRed}[${DGrey} $(date) ${BRed}]
    ${DGrey}|â”€â”€${BRed}[${DGrey} Current Date ${BRed}]${BGreen}â”€â”€â•¼ ${BRed}[${DGrey} ${CurrentDate} ${BRed}]
    ${DGrey}|â”€â”€${BRed}[${DGrey} Starting Time ${BRed}]${BGreen}â”€â”€â•¼ ${BRed}[${DGrey} ${StartingTime} ${BRed}]
    ${DGrey}|
    ${DGrey}|â”€â”€${BRed}[ ğŸ¯ ]${DGrey}â”€â”€${BRed}[${DGrey}$(FunTargetIP)${BRed}]${DGrey}â”€â”€â•¼  ${LRed}IP${BBlue}: ${BGreen}${targetIP}
    ${DGrey}|â”€â”€${BRed}[ ğŸ¯ ]${DGrey}â”€â”€${BRed}[${DGrey}$(FunTargetDns)${BRed}]${DGrey}â”€â”€â•¼  ${LRed}Dns${BBlue}: ${BGreen}${targetDns}
    ${DGrey}|â”€â”€${BRed}[ ğŸ¯ ]${DGrey}â”€â”€${BRed}[${DGrey}$(FunTargetName)${BRed}]${DGrey}â”€â”€â•¼  ${LRed}Name${BBlue}: ${BGreen}${targetNAME}
    ${DGrey}|â”€â”€${BRed}[ ğŸ¯ ]${DGrey}â”€â”€${BRed}[${DGrey}$(FunTargetEmail)${BRed}]${DGrey}â”€â”€â•¼  ${LRed}Email${BBlue}: ${BGreen}${targetEmail}
    ${DGrey}|â”€â”€${BRed}[ ğŸ¯ ]${DGrey}â”€â”€${BRed}[${DGrey}$(FunTargetPhone)${BRed}]${DGrey}â”€â”€â•¼  ${LRed}Phone${BBlue}: ${BGreen}${targetPhone}
    ${DGrey}|
    ${DGrey}â””â”€â”€${BRed}[${DGrey} I ${BRed}]${BGreen}â”€â”€â•¼ ${BRed}[${DGrey} Install HacKingPro On the System ${BRed}]

            ")

    HacKingProMenuMain=$(echo "${BRed}"
    echo "$HacKingProTargetStatus""
    ${DGrey}â”Œâ”€â”€${BRed}[${BYellow} Cover Me ${BRed}]
    ${DGrey}|
    ${DGrey}|â”€â”€${BRed}[${DGrey} 01 ${BRed}]${BGreen}â”€â”€ ${BRed}[${DGrey} Clear logs for user ${BRed}$USER ${BRed}]
    ${DGrey}|â”€â”€${BRed}[${DGrey} 02 ${BRed}]${BGreen}â”€â”€ ${BRed}[${DGrey} Permenently disable auth & bash history ${BRed}]
    ${DGrey}|â”€â”€${BRed}[${DGrey} 03 ${BRed}]${BGreen}â”€â”€ ${BRed}[${DGrey} Restore settings to default ${BRed}]
    ${DGrey}|
    ${DGrey}â””â”€â”€${BRed}[${DGrey} X ${BRed}] ğŸ¤´ Exit / Quit / Close

    ${BGreen}â”Œâ”€â”€${BRed}[${BCyan} Arsenal ${BRed}]${BGreen}â”€â”€â•¼ ${BRed}[${BCyan} Cheat Sheets ${BRed}]${BGreen}â”€â”€â•¼ ${BRed}[${BGreen} Command ${BRed}]${BGreen}â”€â”€â•¼ ${BRed}[${LGreen} $ArsenalCommand ${BRed}]
    ${BGreen}|
    ${BGreen}|â”€â”€${BRed}[${DGrey} ~$(pwd) ${BRed}]
    ${BGreen}|
    ${BGreen}â””â”€â”€${BRed}[${BYellow} $Anlominus ${BRed}]${BGreen}â”€â”€â•¼ ${BRed}[${BGreen} ${HacKingPro} ${BRed}]${BGreen}â”€â”€â•¼ ${BRed}[${BGreen} Anonimity ${BRed}]${BGreen}â”€â”€â•¼  ${BRed}[${BGreen} Cover Me ${BRed}]${BGreen}â”€â”€â•¼ ${BGreen}   ")
            read -p "$HacKingProMenuMain" cmd
            case $cmd in
              1|01 )
              echo " "
              clearLogs
              clearHistory
              ;;
              2|02 )
              echo " "
              disableAuth
              disableHistory
              ;;
              3|03 )
              echo " "
              enableAuth
              enableHistory
              ;;
              lockheed|Lockheed )
              Lockheed
              ;;
              x|X|exit|quit|Exit )
              clear
              HacKingPro=$(echo "${BRed}Hac${BYellow}King${BBlue}Pro")
              echo "\n\t\t ${BRed}[ğŸ™ğŸ¼${BRed}] ${BWhite}Exit ${HacKingPro} ${BRed}[ğŸ™ğŸ¼${BRed}]\n\t\t ${BGreen}"
              break
              ;;
            esac
          done
        }

    clear # Clear output

    # "now" option
    if [ -n "$1" ] && [ "$1" == 'now' ]; then
            clearLogs
            clearHistory
            exit 0
    fi

    MainMenu

    if [ $option == 1 ]; then
            # Clear logs & current history
            clearLogs
            clearHistory
    elif [ $option == 2 ]; then
            # Permenently disable auth & bash log
            disableAuth
            disableHistory
    elif [ $option == 3 ]; then
            # Restore default settings
            enableAuth
            enableHistory
    elif [ $option == 99 ]; then
            # Exit tool
            exitTool
    else
            echo "[!] Option not reconized. Exiting."
    fi
}

