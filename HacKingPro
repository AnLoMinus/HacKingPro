    ${BGreen}â”Œâ”€â”€${BRed}[${BCyan} Version ${BRed}]${BGreen}â”€â”€â”€â”€â”€â”€â•¼ ${BRed}[${BCyan} ${MyVersion} ${BRed}]
    ${BGreen}|â”€â”€${BRed}[${BCyan} Aouther ${BRed}]${BGreen}â”€â”€â”€â”€â”€â”€â•¼ ${BRed}[${BCyan} ${Anlominus} ${BRed}]
    ${BGreen}|â”€â”€${BRed}[${BCyan} Tool Name ${BRed}]${BGreen}â”€â”€â”€â”€â•¼ ${BRed}[${BCyan} ${HacKingPro} ${BRed}]
    ${BGreen}|â”€â”€${BRed}[${BCyan} Last Update ${BRed}]${BGreen}â”€â”€â•¼ ${BRed}[${BCyan} ${LastUPDATE} ${BRed}]
    ${BGreen}â””â”€â”€${BRed}[${BCyan} Source ${BRed}]${BGreen}â”€â”€â”€â”€â”€â”€â”€â•¼ ${BRed}[${DGrey} ${ToolSource} ${BRed}]")

